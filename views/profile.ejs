 <section id="speakers" class="speakers-gallery-sec position-relative">
   <div class="container">
      <div class="section-head col-xl-8 m-auto text-center mb-5">
         <span class="label">Create a new Character Project</span>
         <h2 class="title">
            Here is where you can add your next character to list of cosplays.
         </h2>
      </div>
      <div class="speakers-gallery-items-wrap">
        
         <div class="row">
            <% for(var i=0; i<posts.length; i++) {%>
            <div class="col-md-6 col-lg-4 col-xl-3">
               <div class="speakers-gallery-item" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="500">
              
                  <div class="speakers-gallery-item-thumb overflow-hidden position-relative">
                    <img class="img-fluid" src="<%= posts[i].image%>">
                  </div>
                  <ul class="social-icons social">
                     <li>
                        <a href="/post/<%= posts[i]._id%>">Work</a>
                     </li>
                     <li>
                        <a href="">Edit</a>
                     </li>
                     <li>
                        <a href=""><i class="fa fa-trash" aria-hidden="true"></i></a>
                     </li>
                  </ul>
                  <div class="item-content">
                     <h3 class="title"><%= posts[i].title %></h3>
                     <span class="sub"><%= posts[i].caption %></span>
                  </div>
              
               </div>
            </div>
            <% } %>
         </div>
         
       
         <div class="dots img-moving-anim1">
            <img src="assets/images/dots/dots8.png" alt="Shadow Image">
         </div>
      </div>
      <div>
         <form action="/post/createPost" enctype="multipart/form-data" method="POST">
            <div class="mb-3">
               <label for="imgUpload" class="form-label">Image</label>
               <input type="file" class="form-control" id="imageUpload" name="file">
             </div>
           <div class="mb-3">
               <label for="title" class="form-label">Name</label>
               <input type="text" class="form-control" id="title" name="title">
           </div>
           <div class="mb-3">
             <label for="caption" class="form-label">Media</label>
             <textarea class="form-control" id="caption" name="caption"></textarea>
           </div>
           <button type ="submit" class="custom-btn custom-btn2" value = "Upload">+ Speakers</button>
         </form>
       </div>

   </div>
   <div class="shape">
      <img src="assets/images/shape/3.svg" alt="">
   </div>
</section>

<div class="container">
   <hr>
</div>
 

